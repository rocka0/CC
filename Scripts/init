#!/usr/bin/python

from os import system

ROOT_PATH = "/home/tusky/Tushar/CC"

files = [
	'compile_flags.txt',
	'.clang-format',
	'Snippets/boo.h',
	'Scripts/parse',
	'Scripts/compile',
	'Scripts/run',
	'Scripts/gen',
	'Scripts/stress_test'
]

def main():
	for file in files:
		system(f"cp {ROOT_PATH}/{file} .")
	system("rm ./init")

	return 0

if __name__ == "__main__":
	main()
